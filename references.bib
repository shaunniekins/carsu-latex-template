% --- Article Example ---
% Use @article for articles published in peer-reviewed journals.
% Common fields: author, title, journal, year, volume, number, pages, month, doi, url.
@article{placeholderArticle2023,
  author  = {Author, First A. and Author, Second B.},
  title   = {Title of the Journal Article},
  journal = {Name of the Journal},
  year    = {2023},
  volume  = {10},
  number  = {2},
  pages   = {100--120},
  month   = {June},
  doi     = {10.xxxx/journal.xxxxxx},
  url     = {https://example.com/article}
}

% --- InProceedings Example ---
% Use @inproceedings for papers published in conference proceedings.
% Common fields: author, title, booktitle, year, editor, volume/number, series, pages, address, month, organization, publisher, doi, url.
@inproceedings{placeholderConference2023,
  author    = {Author, Third C. and Author, Fourth D.},
  title     = {Title of the Conference Paper},
  booktitle = {Proceedings of the Example Conference (EXAMPLE '23)},
  year      = {2023},
  pages     = {50--65},
  publisher = {Publisher Name},
  address   = {City, Country},
  month     = {October},
  doi       = {10.xxxx/conference.xxxxxx},
  url       = {https://example.com/conference_paper}
}

% --- Book Example ---
% Use @book for a published book.
% Common fields: author/editor, title, publisher, year, address, volume/number, series, edition, month, isbn.
@book{placeholderBook2023,
  author    = {Author, Fifth E.},
  title     = {Title of the Book},
  publisher = {Academic Press},
  year      = {2023},
  address   = {New York, NY},
  edition   = {3rd},
  isbn      = {978-x-xxxx-xxxx-x}
}

% --- TechReport Example ---
% Use @techreport for technical reports issued by institutions.
% Common fields: author, title, institution, year, type (e.g., "Technical Report"), number, address, month, url.
@techreport{placeholderTechReport2023,
  author      = {Author, Sixth F. and Author, Seventh G.},
  title       = {Title of the Technical Report},
  institution = {Name of the Institution or University},
  year        = {2023},
  type        = {Technical Report},
  number      = {TR-123},
  address     = {City, State},
  month       = {March},
  url         = {https://example.com/techreport}
}

% --- PhD Thesis Example ---
% Use @phdthesis for doctoral dissertations.
% Common fields: author, title, school, year, type (usually "{PhD} dissertation"), address, month, url.
@phdthesis{placeholderPhdThesis2023,
  author  = {Author, Eighth H.},
  title   = {Title of the PhD Dissertation},
  school  = {University Name},
  year    = {2023},
  type    = {{PhD} dissertation},
  address = {City, Country},
  month   = {May},
  url     = {https://example.com/phdthesis}
}

% --- Master's Thesis Example ---
% Use @mastersthesis for master's theses.
% Common fields: author, title, school, year, type (usually "{Master's} thesis"), address, month, url.
@mastersthesis{placeholderMastersThesis2023,
  author  = {Author, Ninth I.},
  title   = {Title of the Master's Thesis},
  school  = {University Name},
  year    = {2023},
  type    = {{Master's} thesis},
  address = {City, Country},
  month   = {August},
  url     = {https://example.com/mastersthesis}
}

% --- Online Resource / Website Example ---
% Use @online for web pages or online resources where other types don't fit well. @misc is also often used.
% Common fields: author/organization, title, year, url, urldate (access date). Month/day can also be added.
@online{placeholderOnline2023,
  author  = {Organization Name or Author, Tenth J.},
  title   = {Title of the Web Page or Online Resource},
  year    = {2023},
  url     = {https://example.com/online_resource},
  urldate = {2024-01-15},
  month   = {July},
  day     = {10}
}

% --- Misc Example (Catch-all) ---
% Use @misc for sources that don't fit other categories (e.g., unpublished work, presentations, personal communications - though check style guide for latter).
% Common fields: author/organization, title, howpublished, month, year, note, url.
% 'howpublished' can contain a URL or description. 'note' provides context.
@misc{placeholderMisc2023,
  author       = {Author, Eleventh K.},
  title        = {Title of the Miscellaneous Resource (e.g., Presentation Slides)},
  howpublished = {Presented at the Example Workshop},
  year         = {2023},
  month        = {November},
  note         = {Unpublished manuscript or specific context.}
}

% --- Comparison Summary ---
% @article: Peer-reviewed journal publications. Key: journal, volume, number, pages.
% @inproceedings: Conference papers in proceedings. Key: booktitle, publisher.
% @book: Published books. Key: publisher, edition, isbn.
% @techreport: Institutional technical reports. Key: institution, type, number.
% @phdthesis: Doctoral dissertations. Key: school, type="{PhD} dissertation".
% @mastersthesis: Master's theses. Key: school, type="{Master's} thesis".
% @online: Web pages/online resources. Key: url, urldate.
% @misc: Catch-all (reports, websites, etc.). Key: howpublished, note.